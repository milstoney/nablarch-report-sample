#===============================================================================
# バッチ固有の設定ファイル
#===============================================================================
# バッチリクエストテーブル
requestTable=BATCH_REQUEST

# バッチリクエストIDカラム名
requestIdColumnName=REQUEST_ID

# 正常に処理できたポイントのカラム名
resumePointColumnName=RESUME_POINT

# デフォルトのスレッド数
threadCount=1

# デフォルトの最大処理件数
maxCount=0

# デフォルトのコミット間隔
commitInterval=1

# コードの初期ロード設定
# 初期ロードは行わない。
loadCodeOnStartUp=false

# メッセージの初期ロード設定
# 初期ロードは行わない。
loadMessageOnStartUp=false

# 業務日付のデフォルト区分
businessDateDefaultSegment=02

# validation時に精査対象の値にnullを許可するか否か
validationConvertorAllowNullValue=true

#===============================================================================
# 常駐バッチ用の設定
#===============================================================================
# データ監視間隔(デフォルトは1秒)
dataWatchInterval=1000
